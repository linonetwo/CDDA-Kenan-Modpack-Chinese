msgid_plural "clay oil lamps"
msgstr[0] "глиняная масляная лампа"
msgstr[1] "глиняных масляных лампы"
msgstr[2] "глиняных масляных ламп"

#. ~ Use action menu_text for {'str': 'clay oil lamp'}.
#: .\items\tools.json
